generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Event {
  id          Int      @id @default(autoincrement())
  createdAt   DateTime @default(now())
  description String
  endTime     String
  eventDate   DateTime
  eventName   String
  location    String
  startTime   String
  tags        Json
  updatedAt   DateTime @updatedAt
}
